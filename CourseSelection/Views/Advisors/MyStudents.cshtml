@model List<CourseSelection.Models.Student>

<h1>My Students</h1>
<table>
    <thead>
        <tr>
            <th>Student ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Enrollment Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.StudentID</td>
                <td>@student.FirstName</td>
                <td>@student.LastName</td>
                <td>@student.Email</td>
                <td>@student.EnrollmentDate.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>


